@model List<Projet2.Models.Stuff>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
@{
    Layout = "_LayoutGeneral";
}

<h2>Catalogue de Stuff</h2>

<div>
    <table border="2">
        <tr>
            <th><h2>Nom</h2></th>
            <th><h2>Description</h2></th>
            <th><h2>Type</h2></th>
            <th><h2>Etat</h2></th>
        </tr>
        @foreach (var stuff in Model)
        {
            <tr>

                <td style="text-align : center">@stuff.Name</td>
                <td style="text-align : center">@stuff.Description</td>
                <td style="text-align : center">@stuff.Type</td>
                <td style="text-align : center">@stuff.State</td>
            </tr>
        }
    </table>

</div>
